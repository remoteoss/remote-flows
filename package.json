{
  "name": "@remoteoss/remote-flows",
  "version": "1.2.2",
  "repository": {
    "type": "git",
    "url": "https://github.com/remoteoss/remote-flows.git"
  },
  "scripts": {
    "build": "NODE_ENV=production tsup",
    "ci": "npm run build && npm run check-format && npm run check-exports && npm run lint && npm run type-check && npm run test",
    "check-exports": "attw --pack . --ignore-rules=cjs-resolves-to-esm --exclude-entrypoints index.css --profile node16",
    "check-format": "prettier --check .",
    "dev": "NODE_OPTIONS='--max-old-space-size=8192' NODE_ENV=development tsup --watch",
    "build:reset": "postcss src/styles/reset.css -o ./example/src/css/reset.css",
    "lint": "eslint .",
    "format": "prettier --write .",
    "openapi-ts": "openapi-ts",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "prepare": "npm run build",
    "release": "tsx scripts/release.ts",
    "size": "tsx scripts/analyze-bundle-size.ts",
    "size:check": "tsx scripts/check-size-limits.ts",
    "size:badge": "tsx scripts/update-size-badge.ts",
    "type-check": "tsc --noEmit"
  },
  "engines": {
    "node": ">=24"
  },
  "type": "module",
  "main": "dist/index.js",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js"
      },
      "default": "./dist/index.js"
    },
    "./internals": {
      "import": {
        "types": "./dist/internals.d.ts",
        "default": "./dist/internals.js"
      },
      "default": "./dist/internals.js"
    },
    "./flows/*": {
      "import": {
        "types": "./dist/flows/*.d.ts",
        "import": "./dist/flows/*.js"
      },
      "default": "./dist/flows/*.js"
    },
    "./index.css": {
      "import": {
        "import": "./dist/index.css"
      },
      "default": "./dist/index.css"
    }
  },
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "react": "^18 || ^19",
    "react-dom": "^18 || ^19"
  },
  "dependencies": {
    "@hookform/resolvers": "^4.1.3",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@remoteoss/json-schema-form-old": "npm:@remoteoss/json-schema-form@0.11.10-beta.0",
    "@remoteoss/remote-json-schema-form-kit": "github:remoteoss/remote-json-schema-form-kit#update-types-kit",
    "@tailwindcss/cli": "^4.1.17",
    "@tailwindcss/postcss": "^4.1.17",
    "@tanstack/react-query": "^5.90.10",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^3.6.0",
    "dompurify": "^3.3.0",
    "lodash.capitalize": "^4.2.1",
    "lodash.get": "^4.4.2",
    "lodash.groupby": "^4.6.0",
    "lodash.isnull": "^3.0.0",
    "lodash.mergewith": "^4.6.2",
    "lodash.omit": "^4.5.0",
    "lodash.omitby": "^4.6.0",
    "lucide-react": "0.477.0",
    "pdfjs-dist": "^5.4.296",
    "postcss": "^8.5.3",
    "react-day-picker": "^8.10.1",
    "react-flagpack": "^2.0.6",
    "react-hook-form": "^7.54.2",
    "react-pdf": "^10.2.0",
    "tailwind-merge": "^3.0.2",
    "tailwindcss": "^4.0.9",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^1.1.2",
    "yup": "^0.32.0"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.18.2",
    "@eslint/js": "^9.39.1",
    "@hey-api/openapi-ts": "^0.88.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jsdom": "^21.1.7",
    "@types/lodash.capitalize": "^4.2.9",
    "@types/lodash.get": "^4.4.9",
    "@types/lodash.groupby": "^4.6.9",
    "@types/lodash.isnull": "^3.0.9",
    "@types/lodash.mergewith": "^4.6.9",
    "@types/lodash.omit": "^4.5.9",
    "@types/lodash.omitby": "^4.6.9",
    "@types/node": "^22.19.1",
    "@types/react": "^19.2.6",
    "@vitest/coverage-istanbul": "^4.0.13",
    "chalk": "^5.3.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.24",
    "filesize": "^10.1.0",
    "globals": "^16.5.0",
    "gzip-size": "^7.0.0",
    "glob": "^10.3.0",
    "jsdom": "^26.1.0",
    "msw": "^2.12.2",
    "prettier": "^3.6.2",
    "tsup": "^8.5.1",
    "tsx": "^4.19.2",
    "typescript-eslint": "^8.47.0",
    "vitest": "^4.0.13"
  }
}
