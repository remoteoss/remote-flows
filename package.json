{
  "name": "@remoteoss/remote-flows",
  "version": "0.10.0",
  "scripts": {
    "build": "NODE_ENV=production tsup",
    "ci": "npm run build && npm run check-format && npm run check-exports && npm run lint && npm run test",
    "check-exports": "attw --pack . --ignore-rules=cjs-resolves-to-esm --exclude-entrypoints index.css --profile node16",
    "check-format": "prettier --check .",
    "dev": "NODE_ENV=development tsup --watch",
    "build:reset": "postcss src/styles/reset.css -o ./example/src/css/reset.css",
    "lint": "eslint .",
    "format": "prettier --write .",
    "openapi-ts": "openapi-ts",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "changeset:add": "changeset add",
    "changeset:version": "changeset version",
    "prepare": "npm run build",
    "prerelease": "npm run ci",
    "release": "changeset publish",
    "type-check": "tsc --noEmit"
  },
  "engines": {
    "node": ">=16"
  },
  "type": "module",
  "main": "dist/index.js",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js"
      },
      "default": "./dist/index.js"
    },
    "./internals": {
      "import": {
        "types": "./dist/internals.d.ts",
        "default": "./dist/internals.js"
      },
      "default": "./dist/internals.js"
    },
    "./flows/*": {
      "import": {
        "types": "./dist/flows/*.d.ts",
        "import": "./dist/flows/*.js"
      },
      "default": "./dist/flows/*.js"
    },
    "./index.css": {
      "import": {
        "import": "./dist/index.css"
      },
      "default": "./dist/index.css"
    }
  },
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "react": "^18 || ^19",
    "react-dom": "^18 || ^19"
  },
  "dependencies": {
    "@hey-api/client-fetch": "^0.8.1",
    "@hookform/resolvers": "^4.1.3",
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-dialog": "^1.1.13",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-popover": "^1.1.6",
    "@radix-ui/react-radio-group": "^1.2.3",
    "@radix-ui/react-scroll-area": "^1.2.3",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-separator": "^1.1.2",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-tabs": "^1.1.3",
    "@radix-ui/react-tooltip": "^1.1.8",
    "@radix-ui/react-collapsible": "1.1.2",
    "@remoteoss/json-schema-form": "0.11.10-beta.0",
    "@tailwindcss/cli": "^4.0.9",
    "@tailwindcss/postcss": "^4.0.9",
    "@tanstack/react-query": "^5.67.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^3.6.0",
    "dompurify": "^3.2.5",
    "lodash.capitalize": "^4.2.1",
    "lodash.get": "^4.4.2",
    "lodash.groupby": "^4.6.0",
    "lodash.isnull": "^3.0.0",
    "lodash.mergewith": "^4.6.2",
    "lodash.omit": "^4.5.0",
    "lodash.omitby": "^4.6.0",
    "lucide-react": "0.477.0",
    "postcss": "^8.5.3",
    "react-day-picker": "^8.10.1",
    "react-hook-form": "^7.54.2",
    "recharts": "^2.15.1",
    "tailwind-merge": "^3.0.2",
    "tailwindcss": "^4.0.9",
    "tailwindcss-animate": "^1.0.7",
    "type-fest": "^4.35.0",
    "vaul": "^1.1.2",
    "yup": "^0.32.0"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.17.3",
    "@changesets/cli": "^2.28.1",
    "@eslint/js": "^9.22.0",
    "@hey-api/openapi-ts": "^0.64.4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jsdom": "^21.1.7",
    "@types/lodash.capitalize": "^4.2.9",
    "@types/lodash.get": "^4.4.9",
    "@types/lodash.groupby": "^4.6.9",
    "@types/lodash.isnull": "^3.0.9",
    "@types/lodash.mergewith": "^4.6.9",
    "@types/lodash.omit": "^4.5.9",
    "@types/lodash.omitby": "^4.6.9",
    "@types/node": "^22.13.4",
    "@types/react": "^19.0.10",
    "@vitest/coverage-istanbul": "^3.0.8",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "jsdom": "^26.0.0",
    "msw": "^2.7.3",
    "prettier": "^3.5.1",
    "tsup": "^8.4.0",
    "typescript-eslint": "^8.26.1",
    "vitest": "^3.0.8"
  }
}
